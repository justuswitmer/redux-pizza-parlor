  "proxy": "http://localhost:5000",
  "dependencies": {
    "axios": "^0.21.1",
    "pg": "^8.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
